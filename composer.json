{
    "name": "caiobas/slim-framework",
    "description": "Slim Framework",
    "type": "project",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.7",
        "slim/psr7": "^1.3",
        "swiftmailer/swiftmailer": "^6.0",
        "php-di/php-di": "^6.3",
        "symfony/dotenv": "^5.2",
        "tuupola/slim-basic-auth": "^3.3",
        "php-amqplib/php-amqplib": "^3.0",
        "ext-json": "*",
        "doctrine/orm": "^2.16",
        "symfony/cache": "^6.0",
        "doctrine/annotations": "^2.0",
        "uma/dic": "^2.0",
        "doctrine/migrations": "^3.5",
        "kekos/doctrine-console-factory": "^1.1",
        "tuupola/slim-jwt-auth": "^3.7",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/http-foundation": "^6.0"
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
